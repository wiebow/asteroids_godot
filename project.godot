config_version=4

_global_script_classes=[ {
"base": "Area2D",
"class": "Player",
"language": "GDScript",
"path": "res://objects/player/player.gd"
} ]
_global_script_class_icons={
"Player": ""
}

[application]

config/name="MyAsteroids"
config/description="A simple asteroid test game in Godot"
run/main_scene="res://main.tscn"
config/icon="res://objects/asteroid/rocks_med.png"

[autoload]

Events="*res://autoload/events.gd"
SceneSwitcher="*res://autoload/scene_switcher.gd"
Score="*res://autoload/score.gd"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=800
window/size/test_width=1600
window/size/test_height=1200
window/vsync/vsync_via_compositor=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[editor_plugins]

enabled=PoolStringArray(  )

[gdnative]

singletons=[  ]


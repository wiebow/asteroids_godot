[application]

config/name="MyAsteroids"
config/description="A simple asteroid test game in Godot"
run/main_scene="res://main/Main.tscn"
config/icon="res://objects/asteroid/rocks_med.png"

[autoload]

Switcher="*res://autoload/Switcher.gd"
Game="*res://autoload/Global.gd"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=800
window/size/test_width=800
window/size/test_height=600
window/vsync/vsync_via_compositor=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

